						Sessions 
						{{!-- <a class="title-link" href="booking.html">+ Buat janji</a> --}}
					</h1>

					{{!-- <a href="booking.html" class="button -primary title-button">+ Buat janji kunjungan</a> --}}

					{{>list mysession=true as_expert=true header=true index_start=0 index_end=13}}

					<div class="board-more">
						<button class="button -primary -auto">Load more</button>
					</div>

				</div>

			</section>

		</main>

		<!-- footer -->
		{{>footer brand="pion-clinician"}}

	</div>

{{>js}}
</body>
</html>
